<!-- 可用组件：https://mp.weixin.qq.com/debug/wxadoc/dev/component/ -->
<view class="container">
  <text class="title">{{ title }}</text>
</view>
<view style='height: 138px;'>
  <image src='../../images/mobile.png' style='width:100%; height:100%;'></image>
</view>
<view bindtap='start'>start</view>
<canvas canvas-id='one'></canvas>
<view bindtap='stop' style='width:50px; height:50px; backgroundColor:#f00;'>stop</view>

<view class='test-out'>
  <view class='test'>
    <image src='{{ showImage }}' mode='{{ imageMode }}'></image>
  </view>
</view>
